{
  "name": "morozov/diff-sniffer-pull-request",
  "description": "GitHub pull request validator for Diff Sniffer",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "require": {
    "morozov/diff-sniffer-core": "dev-phpcs-15",
    "knplabs/github-api": "dev-master"
  },
  "autoload": {
    "files": ["src/diff-sniffer.php"],
    "psr-0": {
      "DiffSniffer": "src/"
    }
  },
  "bin": [
    "bin/authenticate",
    "bin/pull-request"
  ],
  "minimum-stability": "dev"
}
